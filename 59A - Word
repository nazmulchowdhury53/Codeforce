#include<bits/stdc++.h>
#include<math.h>
#include<vector>
#include<string.h>
using namespace std;
int main()
{

    long long int a=0,b=0,c,d,e,u,i,j;
    string s;
    cin>>s;
    c = s.length();
      for (i = 0; i <c; i++) {
        if (isupper(s[i])) {
            a++;
        } else {
            b++;
        }
    }
    if (a> b) {
        for (i = 0; i <c; i++) {
            s[i] = toupper(s[i]);
        }
    } else {
        for (i = 0; i < c; i++) {
            s[i] = tolower(s[i]);
        }
    }

   cout<<s<<endl;
  
}
